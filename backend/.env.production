# Production Environment Variables
NODE_ENV=production
PORT=3000

# CORS Configuration - Allow multiple origins
CORS_ENABLED=true
CORS_ORIGINS=["http://localhost:3000","http://localhost:3001","https://connect-four-ai-derek.vercel.app","https://connect-four-ai-roge.vercel.app","https://*.vercel.app"]

# Frontend URLs
FRONTEND_URL=https://connect-four-ai-derek.vercel.app

# ML Service (disabled in production for now)
ML_SERVICE_URL=http://localhost:8000
ML_SERVICE_ENABLED=false

# Socket.IO Settings
SOCKET_IO_TRANSPORTS=["polling","websocket"]

# Enterprise Features
ENTERPRISE_MODE=true
ENABLE_ADVANCED_AI=true
