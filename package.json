{
  "name": "connect-four-monorepo",
  "private": true,
  "scripts": {
    "start": "node scripts/unified-enterprise-launcher.js --profile development",
    "start:minimal": "node scripts/unified-enterprise-launcher.js --profile minimal",
    "start:development": "node scripts/unified-enterprise-launcher.js --profile development",
    "start:production": "node scripts/unified-enterprise-launcher.js --profile production",
    "start:testing": "node scripts/unified-enterprise-launcher.js --profile testing",
    "start:enterprise": "node scripts/unified-enterprise-launcher.js --profile enterprise",
    "start:turbo": "node scripts/unified-enterprise-launcher.js --profile development",
    "start:turbo:build": "node scripts/unified-enterprise-launcher.js --profile production",
    "start:fast": "node scripts/unified-enterprise-launcher.js --profile minimal",
    "start:force": "node scripts/enterprise-parallel-launcher.js",
    "start:backend": "./smart-start.sh --backend-only",
    "start:frontend": "./smart-start.sh --frontend-only",
    "start:follow": "./smart-start.sh --follow-logs",
    "dev": "node scripts/unified-enterprise-launcher.js --profile development",
    "dev:turbo": "node scripts/unified-enterprise-launcher.js --profile development",
    "dev:turbo:build": "node scripts/unified-enterprise-launcher.js --profile production",
    "dev:fast": "node scripts/unified-enterprise-launcher.js --profile minimal",
    "dev:enterprise": "node scripts/unified-enterprise-launcher.js --profile enterprise",
    "dev:clean": "node scripts/intelligent-resource-manager.js --cleanup",
    "dev:logs": "./smart-start.sh --follow-logs",
    "stop": "pkill -f 'unified-enterprise-launcher\\|enterprise-parallel-launcher\\|ai-stability-manager\\|intelligent-resource-manager'; ./smart-stop-parallel.sh",
    "stop:turbo": "pkill -f 'unified-enterprise-launcher\\|enterprise-parallel-launcher\\|ai-stability-manager\\|intelligent-resource-manager'; ./smart-stop-parallel.sh",
    "stop:enterprise": "pkill -f 'node scripts/(unified-enterprise-launcher|enterprise-parallel-launcher|ai-stability-manager|intelligent-resource-manager|performance-analytics-suite|advanced-deployment-manager|ai-comprehensive-testing|enterprise-model-manager|advanced-ai-diagnostics|rlhf-system-manager|ai-orchestration-dashboard)'",
    "stop:legacy": "./port-manager.sh cleanup-force",
    "stop:parallel": "./smart-stop-parallel.sh",
    "stop:interactive": "./port-manager.sh cleanup",
    "stop:force": "pkill -f 'node scripts/'; ./port-manager.sh cleanup-force",
    "stop:gentle": "./smart-stop-parallel.sh",
    "restart": "npm run stop && npm run start",
    "restart:turbo": "npm run stop:turbo && npm run start:turbo",
    "restart:turbo:build": "npm run stop:turbo && npm run start:turbo:build",
    "restart:fast": "npm run stop && npm run start:fast",
    "restart:enterprise": "npm run stop:enterprise && npm run start:enterprise",
    "status": "ps aux | grep -E 'unified-enterprise-launcher|enterprise-parallel-launcher|ai-stability-manager' | grep -v grep",
    "status:detailed": "./port-manager.sh status",
    "status:enterprise": "ps aux | grep -E 'node scripts/(unified-enterprise-launcher|enterprise-parallel-launcher|ai-stability-manager|intelligent-resource-manager|performance-analytics-suite|advanced-deployment-manager)' | grep -v grep",
    "scan": "./port-manager.sh scan",
    "kill-port": "./port-manager.sh kill",
    "kill-nest": "node scripts/intelligent-resource-manager.js --kill-processes",
    "emergency": "npm run stop:force",
    "emergency:ports": "./port-manager.sh cleanup-force",
    "cleanup": "node scripts/intelligent-resource-manager.js --cleanup",
    "cleanup:force": "npm run stop:force",
    "train": "./enhanced-pipeline.sh",
    "train:help": "./enhanced-pipeline.sh --help",
    "train:offline": "./enhanced-pipeline.sh offline",
    "train:continuous": "./enhanced-pipeline.sh continuous",
    "train:api": "./enhanced-pipeline.sh api",
    "demo": "node scripts/performance-analytics-suite.js",
    "demo:performance": "node scripts/performance-analytics-suite.js --benchmark",
    "demo:analytics": "node scripts/performance-analytics-suite.js --dashboard",
    "demo:testing": "node scripts/ai-comprehensive-testing.js --quick",
    "legacy:start:backend": "cd backend && npm install && npm run start:dev",
    "legacy:start:frontend": "cd frontend && npm install && npm run start",
    "legacy:dev": "concurrently \"npm run legacy:start:backend\" \"npm run legacy:start:frontend\"",
    "enterprise:dashboard": "node scripts/unified-enterprise-launcher.js",
    "enterprise:orchestration": "node scripts/ai-orchestration-dashboard.js",
    "enterprise:stability": "node scripts/ai-stability-manager.js",
    "enterprise:resources": "node scripts/intelligent-resource-manager.js",
    "enterprise:analytics": "node scripts/performance-analytics-suite.js",
    "enterprise:deployment": "node scripts/advanced-deployment-manager.js",
    "enterprise:testing": "node scripts/ai-comprehensive-testing.js",
    "enterprise:models": "node scripts/enterprise-model-manager.js",
    "enterprise:diagnostics": "node scripts/advanced-ai-diagnostics.js",
    "enterprise:rlhf": "node scripts/rlhf-system-manager.js",
    "models:manager": "node scripts/enterprise-model-manager.js",
    "models:cleanup": "node scripts/enterprise-model-manager.js --cleanup",
    "models:deploy": "node scripts/enterprise-model-manager.js --deploy",
    "models:validate": "node scripts/enterprise-model-manager.js --validate",
    "models:monitor": "node scripts/enterprise-model-manager.js --monitor",
    "models:health": "node scripts/enterprise-model-manager.js --health"
  },
  "dependencies": {
    "react-scripts": "^5.0.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/socket.io-client": "^1.4.36"
  }
}